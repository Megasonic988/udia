{
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 2016,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "globals": {
        "document": true,
        "fetch": true,
        "localStorage": true,
        "window": true
    },
    "rules": {
        "no-constant-condition": 0,
        "comma-dangle": ["error", "never"]
    }
}